		<dc:creator><![CDATA[Amit]]></dc:creator>
		<guid isPermaLink="false">https://awnxt.thearks.in/?post_type=m_debugger&#038;p=1232462</guid>
		<description></description>
		<content:encoded><![CDATA[[templates.add main]

 <div class='debug-form'>
  <form class="" 
      id="debug_form" 
      novalidate> 
  	<div class="form-group ">
        <div class="col-md-12 input-group no-padding">
          <label for="debug_code">Debugging Logic<span class="text-danger">*</span></label>
        </div>

				<div class="">
						
            <textarea id="debug_code" name="debug_code" placeholder="" class="form-control-md" rows="20" cols="100" required="" msg-required="<i class='icon icon-warning'></i> Debugging Logic is required!" autocomplete="off" spellcheck="false" style="padding:5px 15px;">[module.get debug_code /]</textarea>
			
				</div>    
        <i class="info-block"></i>
        <div class="help-block"></div>
	</div>
    <input type='hidden' name='ticket_id' value='[module.ticket_id /]'>
    <div class='debug-update'></div>
 	  <button type="submit" class="btn btn-primary btn-rounded" style='margin-top:10px'><i class="fa fa-check icon icon-check"></i> Save and Set Cookie</button>

</form>
 //**debug-code-form**//
  <template 
      type=spa/axn 
      axn='form.ajax' 
      bind='submit' 
      bind_selector='#debug_form' 
      route_ajax='[module.slug/].submit' 
      disable_selector='.btn'>
    </template> 
 	</div>   
[/templates.add]
[templates.add submit]
	[template.set req={request2} /]
	 
   [if.empty request2.ticket_id]
   		[session_ticket.create o.set=template.ticket/]
   [/if.empty]
   [if.else]
   		[template.set ticket="{request2.ticket_id}" /]
   [/if.else]
   [session_ticket.set {template.ticket} field="debug_code" value='{request2.debug_code}' /]
   <template axn=cookie.set get='[template.ticket /]' set='live_debug' hour=true></template>
   <template axn=selector.update selector='.debug-update' get='me.html'> 
   	 
     <script type='spa/axn' axn="core.run_script">
     	window.location.href = "[app.path /]/ticket/[template.get ticket /]"
     </script>
     
     <h3> Cookie set with the debug code. Ticket ID is  [template.get ticket /] </h3>
     
   </template> 
[/templates.add]]]></content:encoded>
